[
  {
    "key": "ctrl+l",
    "command": "settings.cycle",
    "when": "editorTextFocus",
    "args": {
      "id": "relativeLineNumbers",
      "values": [
        {
          "editor.lineNumbers": "on"
        },
        {
          "editor.lineNumbers": "relative"
        }
      ]
    }
  },
  {
    "key": "shift-escape",
    "command": "workbench.action.closePanel"
  }
]
