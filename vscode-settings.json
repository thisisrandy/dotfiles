{
  "terminal.integrated.showExitAlert": false,
  "editor.formatOnSave": true,
  "vim.easymotion": true,
  "vim.sneak": false,
  "vim.surround": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": ["#d4c528", "#303030"],
  "vim.statusBarColors.insert": ["#3fb5d9", "#001761"],
  "vim.statusBarColors.visual": ["#cc6402", "#301800"],
  "vim.statusBarColors.visualline": ["#cc6402", "#301800"],
  "vim.statusBarColors.visualblock": ["#cc6402", "#301800"],
  "vim.statusBarColors.replace": ["#bf0000", "#ffffff"],
  "vim.statusBarColors.commandlineinprogress": ["#31de3c", "#0039c9"],
  "vim.statusBarColors.searchinprogressmode": ["#31de3c", "#0039c9"],
  "vim.statusBarColors.easymotionmode": ["#31de3c", "#0039c9"],
  "vim.statusBarColors.easymotioninputmode": ["#31de3c", "#0039c9"],
  "vim.statusBarColors.surroundinputmode": ["#31de3c", "#0039c9"],
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.joinspaces": false,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "w", "r"],
      "commands": ["editor.action.toggleWordWrap"]
    },
    {
      "before": ["Y"],
      "after": ["y", "$"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": [";"],
      "commands": ["workbench.action.showAllEditors"]
    },
    {
      "before": ["m", "a"],
      "commands": ["numberedBookmarks.toggleBookmark1"]
    },
    {
      "before": ["m", "b"],
      "commands": ["numberedBookmarks.toggleBookmark2"]
    },
    {
      "before": ["m", "c"],
      "commands": ["numberedBookmarks.toggleBookmark3"]
    },
    {
      "before": ["m", "d"],
      "commands": ["numberedBookmarks.toggleBookmark4"]
    },
    {
      "before": ["m", "e"],
      "commands": ["numberedBookmarks.toggleBookmark5"]
    },
    {
      "before": ["m", "f"],
      "commands": ["numberedBookmarks.toggleBookmark6"]
    },
    {
      "before": ["m", "g"],
      "commands": ["numberedBookmarks.toggleBookmark7"]
    },
    {
      "before": ["m", "h"],
      "commands": ["numberedBookmarks.toggleBookmark8"]
    },
    {
      "before": ["m", "i"],
      "commands": ["numberedBookmarks.toggleBookmark9"]
    },
    {
      "before": ["m", "j"],
      "commands": ["numberedBookmarks.toggleBookmark0"]
    },
    {
      "before": ["'", "a"],
      "commands": ["numberedBookmarks.jumpToBookmark1"]
    },
    {
      "before": ["'", "b"],
      "commands": ["numberedBookmarks.jumpToBookmark2"]
    },
    {
      "before": ["'", "c"],
      "commands": ["numberedBookmarks.jumpToBookmark3"]
    },
    {
      "before": ["'", "d"],
      "commands": ["numberedBookmarks.jumpToBookmark4"]
    },
    {
      "before": ["'", "e"],
      "commands": ["numberedBookmarks.jumpToBookmark5"]
    },
    {
      "before": ["'", "f"],
      "commands": ["numberedBookmarks.jumpToBookmark6"]
    },
    {
      "before": ["'", "g"],
      "commands": ["numberedBookmarks.jumpToBookmark7"]
    },
    {
      "before": ["'", "h"],
      "commands": ["numberedBookmarks.jumpToBookmark8"]
    },
    {
      "before": ["'", "i"],
      "commands": ["numberedBookmarks.jumpToBookmark9"]
    },
    {
      "before": ["'", "j"],
      "commands": ["numberedBookmarks.jumpToBookmark0"]
    },
    {
      "before": ["]", "c"],
      "commands": [
        "workbench.action.editor.nextChange",
        "workbench.action.compareEditor.nextChange"
      ]
    },
    {
      "before": ["[", "c"],
      "commands": [
        "workbench.action.editor.previousChange",
        "workbench.action.compareEditor.previousChange"
      ]
    },
    {
      "before": ["<leader>", "h", "u"],
      "commands": ["git.revertSelectedRanges"]
    },
    {
      "before": ["<leader>", "h", "s"],
      "commands": ["git.stageSelectedRanges"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": [":nohl"]
    },
    {
      "before": ["f"],
      "after": ["<leader>", "<leader>", "s"]
    },
    {
      "before": ["/"],
      "after": ["<leader>", "<leader>", "/"]
    },
    {
      "before": ["<leader>", "j"],
      "after": ["<leader>", "<leader>", "j"]
    },
    {
      "before": ["<leader>", "k"],
      "after": ["<leader>", "<leader>", "k"]
    },
    {
      "before": ["<C-h>"],
      "after": [":", "%", "s", "/", "/", "/", "g", "<left>", "<left>", "<left>"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["g", "p"],
      "commands": ["editor.action.marker.prevInFiles"]
    },
    {
      "before": ["g", "n"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.peekImplementation"]
    },
    {
      "before": ["g", "t"],
      "commands": ["editor.action.peekTypeDefinition"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "h", "u"],
      "commands": ["git.revertSelectedRanges"]
    },
    {
      "before": ["<leader>", "h", "s"],
      "commands": ["git.stageSelectedRanges"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": ["<C-h>"],
      "after": [":", "s", "/", "/", "/", "g", "<left>", "<left>", "<left>"]
    }
  ],
  "editor.lineNumbers": "relative",
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.leader": " ",
  "vim.handleKeys": {
    "<C-b>": false,
    "<C-j>": false,
    "<C-q>": false,
    "<C-k>": false,
    "<C-s>": false,
    "<C-f>": false,
    "<C-.>": false
  },
  "editor.acceptSuggestionOnEnter": "off",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.packageManager": "yarn",
  "workbench.iconTheme": "material-icon-theme",
  "vim.easymotionMarkerFontFamily": "'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "vim.easymotionMarkerFontWeight": "bold",
  "explorer.confirmDelete": false,
  "files.trimTrailingWhitespace": true,
  "editor.autoClosingQuotes": "always",
  "editor.autoClosingBrackets": "always",
  "workbench.editor.showTabs": false,
  "zenMode.hideStatusBar": false,
  "files.hotExit": "off",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "workbench.editor.enablePreview": false,
  "editor.tabSize": 2,
  "vsicons.dontShowNewVersionMessage": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.minimap.enabled": false,
  "terminal.integrated.fontFamily": "monospace",
  "python.dataScience.sendSelectionToInteractiveWindow": true,
  "python.dataScience.askForKernelRestart": false,
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "python.jediEnabled": false,
  "python.formatting.provider": "black",
  "vim.timeout": 500,
  "java.configuration.checkProjectSettingsExclusions": false,
  "java.semanticHighlighting.enabled": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.languageServer": "Pylance",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.editorAssociations": [],
  "git.autofetch": true,
  "python.showStartPage": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jest.autoRun": "off",
  "search.exclude": {
    ".venv/*": true
  }
}
