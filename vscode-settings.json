{
  "window.zoomLevel": 0,
  "editor.formatOnSave": true,
  "vim.easymotion": true,
  "vim.sneak": false,
  "vim.surround": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": [
    "#d4c528",
    "#303030"
  ],
  "vim.statusBarColors.insert": [
    "#3fb5d9",
    "#001761"
  ],
  "vim.statusBarColors.visual": [
    "#cc6402",
    "#301800"
  ],
  "vim.statusBarColors.visualline": [
    "#cc6402",
    "#301800"
  ],
  "vim.statusBarColors.visualblock": [
    "#cc6402",
    "#301800"
  ],
  "vim.statusBarColors.replace": [
    "#bf0000",
    "#ffffff"
  ],
  "vim.statusBarColors.commandlineinprogress": [
    "#31de3c",
    "#0039c9"
  ],
  "vim.statusBarColors.searchinprogressmode": [
    "#31de3c",
    "#0039c9"
  ],
  "vim.statusBarColors.easymotionmode": [
    "#31de3c",
    "#0039c9"
  ],
  "vim.statusBarColors.easymotioninputmode": [
    "#31de3c",
    "#0039c9"
  ],
  "vim.statusBarColors.surroundinputmode": [
    "#31de3c",
    "#0039c9"
  ],
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<A-k>"
      ],
      "commands": [
        "editor.action.moveLinesUpAction"
      ]
    },
    {
      "before": [
        "<A-j>"
      ],
      "commands": [
        "editor.action.moveLinesDownAction"
      ]
    },
    {
      "before": [
        "<leader>",
        "/"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "s"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "s"
      ]
    },
    {
      "before": [
        "<leader>",
        "j"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "j"
      ]
    },
    {
      "before": [
        "<leader>",
        "k"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "k"
      ]
    },
    {
      "before": [
        "<leader>",
        "h"
      ],
      "after": [
        ":",
        "%",
        "s",
        "/",
        "/",
        "/",
        "<left>",
        "<left>"
      ]
    },
    {
      "before": [
        "g",
        "r"
      ],
      "commands": [
        "editor.action.referenceSearch.trigger"
      ]
    },
    {
      "before": [
        "<leader>",
        "b"
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "editor.action.showHover"
      ]
    },
    {
      "before": [
        "g",
        "p"
      ],
      "commands": [
        "editor.action.marker.prevInFiles"
      ]
    },
    {
      "before": [
        "g",
        "n"
      ],
      "commands": [
        "editor.action.marker.nextInFiles"
      ]
    },
    {
      "before": [
        "g",
        "i"
      ],
      "commands": [
        "editor.action.peekImplementation"
      ]
    },
    {
      "before": [
        "g",
        "t"
      ],
      "commands": [
        "editor.action.peekTypeDefinition"
      ]
    },
    {
      "before": [
        "<leader>",
        "r"
      ],
      "commands": [
        "editor.action.rename"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "i",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    },
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    },
    {
      "before": [
        "<leader>",
        "h"
      ],
      "after": [
        ":",
        "s",
        "/",
        "/",
        "/",
        "<left>",
        "<left>"
      ]
    }
  ],
  "editor.lineNumbers": "relative",
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "i",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.leader": " ",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-b>": false,
    "<C-j>": false,
    "<C-q>": false,
    "<C-k>": false,
    "<C-s>": false
  },
  "editor.acceptSuggestionOnEnter": "smart",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  }
}
